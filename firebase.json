{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "firestore": {
      "port": "5002"
    },
    "auth": {
      "port": "5003"
    },
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    }
  }
}